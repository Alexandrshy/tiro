    "file-saver": "^2.0.5",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-draggable": "^4.4.3",
    "react-scripts": "4.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
